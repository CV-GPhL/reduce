Import("reduce_scons_env")

env = reduce_scons_env.Copy()
env.Append(
  CXXFLAGS=[
    "-DBOOLPREDEFINED",
    "-DHASSSCANFEXTERN"])
env.StaticLibrary(
  target=["#reduce/lib/pdb++"],
  source=[
    "pdb_read.cpp",
    "pdb_sprntf.cpp",
    "pdb_sscanf.cpp",
    "pdb_chars.cpp",
    "pdb_type.cpp",
    "pdb++.cpp",
    "pdbinput.cpp"])
